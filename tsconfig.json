{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "declaration": true,
    "rootDir": "src",
    "moduleResolution": "node",
  "typeRoots": ["node_modules/@types", "src/types"],
    "esModuleInterop": true,
    "outDir": "dist",
    "noImplicitAny": false,
  "types": ["node", "jest", "axe-playwright"]
  },
  "exclude": ["node_modules"],
  "include": ["src/"]
}
